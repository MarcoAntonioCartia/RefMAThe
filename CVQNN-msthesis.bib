
@article{siemaszko_rapid_2023,
	title = {Rapid training of quantum recurrent neural networks},
	volume = {5},
	issn = {2524-4906, 2524-4914},
	url = {https://link.springer.com/10.1007/s42484-023-00117-0},
	doi = {10.1007/s42484-023-00117-0},
	abstract = {Time series prediction is essential for human activities in diverse areas. A common approach to this task is to harness recurrent neural networks (RNNs). However, while their predictions are quite accurate, their learning process is complex and, thus, time and energy consuming. Here, we propose to extend the concept of RRNs by including continuous-variable quantum resources in it and to use a quantum-enhanced RNN to overcome these obstacles. The design of the continuous-variable quantum RNN (CV-QRNN) is rooted in the continuous-variable quantum computing paradigm. By performing extensive numerical simulations, we demonstrate that the quantum network is capable of learning-time dependence of several types of temporal data and that it converges to the optimal weights in fewer epochs than a classical network. Furthermore, for a small number of trainable parameters, it can achieve lower losses than its classical counterpart. CV-QRNN can be implemented using commercially available quantum-photonic hardware.},
	language = {en},
	number = {2},
	urldate = {2023-11-15},
	journal = {Quantum Machine Intelligence},
	author = {Siemaszko, Michał and Buraczewski, Adam and Le Saux, Bertrand and Stobińska, Magdalena},
	month = dec,
	year = {2023},
	pages = {31},
	file = {Siemaszko et al. - 2023 - Rapid training of quantum recurrent neural network.pdf:/Users/matteoantonioinajetovic/Zotero/storage/YBMFSH2E/Siemaszko et al. - 2023 - Rapid training of quantum recurrent neural network.pdf:application/pdf},
}

