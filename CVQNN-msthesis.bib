	journal = {Physical Review A},
	author = {Deller, Yannick and Schmitt, Sebastian and Lewenstein, Maciej and Lenk, Steve and Federer, Marika and Jendrzejewski, Fred and Hauke, Philipp and Kasper, Valentin},
	month = jun,
